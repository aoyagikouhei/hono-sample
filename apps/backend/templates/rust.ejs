<%
const structName = _.capitalize(_.camelCase(_.pluralize(src.metadata.name)));
const getType = (column) => {
    let innreType = '';
    switch (column.type) {
        case 'string':
            innreType = 'String';
            break;
        case 'number':
            innreType = 'i64';
            break;
    }
    if (column.optional) {
        return `Option<${innreType}>`;
    } else {
        return innreType;
    }
};
%>use serde::{Serialize, Deserialize};

#[derive(Debug, Clone, Serialize, Deserialize)]
pub struct <%- structName %> {<% for (const column of columns) { %>
    pub <%- _.camelCase(column.name) %>: <%- getType(column) %>,<% } %>
}